  "devDependencies": {
    "mocha": "^2.1.0",
    "sinon": "^1.12.2",
    "mocha-term-cov-reporter": "^0.2.0",
    "mocha-lcov-reporter": "0.0.1",
    "blanket": "^1.1.6",
    "jshint": "~2.5.11",
    "coveralls": "~2.11.2"
  },
  "dependencies": {
    "sax": "^0.6.1",
